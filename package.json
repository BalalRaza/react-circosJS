{
  "name": "react-circos",
  "version": "1.0.0",
  "description": "react wrapper for circosJS",
  "main": "build/index.js",
  "repository": "https://github.com/plotly/react-circosJS",
  "author": "Nicolas Girault",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel lib -d build",
    "build:watch": "babel lib/ -w -d build",
    "lint": "eslint lib/**; exit 0",
    "lint:watch": "esw -w lib/**",
    "prepublish": "npm run build",
    "test": "yarn test:units",
    "test:units": "jest lib",
    "test:screenshots": "yarn test:screenshots:build && yarn test:screenshots:run && yarn test:screenshots:upload",
    "test:screenshots:build": "webpack --config tests/webpack.config.js",
    "test:screenshots:run": "node tests/screenshots",
    "test:screenshots:upload": "ARGOS_COMMIT=$CIRCLE_SHA1 ARGOS_BRANCH=$CIRCLE_BRANCH argos upload tests/screenshots --token $ARGOS_TOKEN || true",
    "test:watch": "jest --watch lib"
  },
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "argos-cli": "^0.0.9",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-watch": "^4.0.1",
    "finalhandler": "^1.1.1",
    "jest": "^23.4.1",
    "prop-types": "^15.6.2",
    "puppeteer": "^1.6.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "serve-static": "^1.13.2",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  }
}
